{
  "manifest_version": 3,
  "name": "Cable",
  "version": "1.0",
  "description": "Extracts timestamps from YouTube endscreen recommendations",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ],
  "options_page": "settings.html"
} 